---
import Card from "./card.astro";
const {categories} = Astro.props;
---
{Object.keys(categories).map((category)=>{
  return (
  <div>
    <h1 class="translate-x-0.5 font-round text-white text-xl mb-4">{category}</h1>
    {categories[category].map((items)=>(
      <Card link={items.link} title={items.title} paragraph={items.paragraph} clx={items.clx} boxShadow={items.boxShadow} />
    ))}
  </div>
)})}